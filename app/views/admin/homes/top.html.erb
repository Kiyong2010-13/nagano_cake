<h3>注文履歴一覧</h3>
<table class="table">
  <thead>
    <tr>
      <th>購入日時</th>
      <th>購入者</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.created_at %></td>
        <td><%= order.full_name %></td>
        <td><%= order.email %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>